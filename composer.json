{
  "name": "bytic/controllers-extra",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/bytic/controllers-extra",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\Controllers\\Behaviors\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\Controllers\\Extra\\Tests\\": "tests/src",
      "ByTIC\\Controllers\\Extra\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^8.0",
    "bytic/controllers": "^1.0|^2.0",
    "bytic/utility": "^1.0|^2.0"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0",
    "bytic/dispatcher": "^1.0|^2.0",
    "bytic/orm": "^1.0|^2.0",
    "bytic/orm-filters": "^1.0|^2.0",
    "bytic/models-smart-properties": "^1.0|^2.0",
    "bytic/http": "^1.0|^2.0",
    "bytic/router": "^1.0|^2.0",
    "bytic/form": "^1.0|^2.0",
    "bytic/view": "^1.0|^2.0"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
